{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@components": "./src/components",
          "@theme": "./src/theme",
          "@utils": "./src/utils",
          "@constants": "./src/constants",
          "@assets": "./src/assets",
          "@hooks": "./src/hooks"
        }
      }
    ]
  ]
}